program Urutan;
uses crt; 

var
  a, b, c, temp: integer;
begin
clrscr;
  writeln('Masukkan Nilai 1 :  '); readln(a);
  writeln('Masukkan Nilai 2 :  '); readln(b);
  writeln('Masukkan Nilai 3 :  '); readln(c);
  
  if a < b then
  begin
    temp := a;
    a := b;
    b := temp;
  end;
  
  if a < c then
  begin
    temp := a;
    a := c;
    c := temp;
  end;
  
  if b < c then
  begin
    temp := b;
    b := c;
    c := temp;
  end;
  
  writeln('Urutan: ',a,' ',b,' ',c);
end.
